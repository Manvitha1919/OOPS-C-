#include <iostream>
using namespace std;
int main(){
    int n,num,rem=0,sum=0;
    cout  <<"enter the value of n:";
    cin  >>n;
    num=n;
    while(num>0)
    {
        rem=num%10;
        sum=sum+rem;
        num=num/10;
    }
    if(n%sum==0)
    {
        cout  <<"entered number is a harshad number";
    }
    else
    {
        cout  <<"entered number is not a harshad number";
    }
    return 0;
}
