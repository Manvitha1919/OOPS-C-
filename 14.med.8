#include <iostream>

class Rectangle {
private:
    double length;
    double width;

public:
    // Constructor
    Rectangle() {
        length = 0.0;
        width = 0.0;
    }

    // Member function to set length
    void setLength(double len) {
        length = len;
    }

    // Member function to set width
    void setWidth(double wid) {
        width = wid;
    }

    // Member function to get length
    double getLength() const {
        return length;
    }

    // Member function to get width
    double getWidth() const {
        return width;
    }
};

int main() {
    Rectangle rect;

    // Set length and width of the rectangle
    rect.setLength(5.0);
    rect.setWidth(3.0);

    // Get and print the length and width of the rectangle
    std::cout << "Length: " << rect.getLength() << std::endl;
    std::cout << "Width: " << rect.getWidth() << std::endl;

    return 0;
}
