#include <iostream>
using namespace std;
int main() {
    int dividend, divisor;
    double quotient;
    cout << "Enter the dividend: ";
    cin >> dividend;
    cout << "Enter the divisor: ";
    cin >> divisor;
    if (divisor == 0) {
        cout << "Error: Division by zero is undefined. Please enter a non-zero divisor." << endl;
    } else {
        quotient = static_cast<double>(dividend) / divisor;
        cout << "Result of division: " << quotient << endl;
    }

    return 0;
}
