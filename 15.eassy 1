#include <iostream>
#include <string>

class Car {
public:
    // Constructor initializing model to "Unknown"
    Car() : model("Unknown") {}

    // Method to get the model of the car
    std::string getModel() const {
        return model;
    }

private:
    std::string model;
};

int main() {
    // Create an object of the Car class
    Car myCar;

    // Display the model of the car
    std::cout << "Model of the car: " << myCar.getModel() << std::endl;

    return 0;
}
