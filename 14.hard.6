#include <iostream>

// Function to calculate factorial using call by reference
void calculateFactorial(int n, unsigned long long int &result, int start = 1) {
    result = 1;
    for (int i = start; i <= n; ++i) {
        result *= i;
    }
}

int main() {
    int number;
    unsigned long long int factorial;

    std::cout << "Enter a number to calculate factorial: ";
    std::cin >> number;

    // Calculate factorial using default start value of 1
    calculateFactorial(number, factorial);
    std::cout << "Factorial of " << number << " is: " << factorial << std::endl;

    return 0;
}
