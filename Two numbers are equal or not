#include <iostream>
using namespace std;
int main()
{
    int a,b;
    cout << "enter 1st number:";
    cin>>a;
    cout<< "enter 2nd number: ";
    cin>>b;
    if(a==b)
    {
        cout<< a << " and " <<b << " are equal";
    }
    else
    {
        cout<< a << " and " <<b << " are not equal";
    }
    return 0;
}
